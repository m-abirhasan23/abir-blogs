---
// data imports
import navData from "../data/navdata";
import Link from "./Link.astro";
---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" class="logomark" aria-level="Go home">Abir.Dev</a>
    <ul>
      {
        navData.map((item) => (
          <Link
            style={"primary"}
            href={item.path}
            text={item.name}
            isFilled={false}
            data-navLink
          />
        ))
      }
    </ul>
  </div>
</nav>

<script>
  import "../js/nav.js";
</script>
